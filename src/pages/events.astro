---
import RainbowBanner from "components/RainbowBanner";
import BaseLayout from "layouts/BaseLayout.astro";
---

<BaseLayout title="Events">
  <div class="h-screen flex flex-col">
    <div class="flex-1 h-svh">
      <iframe
        src="https://calendar.google.com/calendar/embed?height=1000&wkst=1&ctz=Europe%2FLondon&mode=AGENDA&hl=en_GB&showPrint=0&src=dGhlaHVicHBjY0BnbWFpbC5jb20&color=%23039be5"
        style="border-width:0; width: 100%; height: 100%; min-height: 100vh;"
        frameborder="0"
        scrolling="yes"></iframe>
    </div>
  </div>
</BaseLayout>
